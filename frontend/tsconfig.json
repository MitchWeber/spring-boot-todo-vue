{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "target": "es6",
        "module": "esnext",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "importHelpers": true,
        "allowJs": true,
        "sourceMap": true,
        "baseUrl": ".",
        "paths": {
            "@Generated/*": [
                "target/generated-sources/*"
            ]
        }
    },
    "include": [
        "src/**/*",
        "target/generated-sources/**/*"
    ]
}
